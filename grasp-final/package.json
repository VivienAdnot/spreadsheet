{
  "name": "grasp-final",
  "description": "Grasp Google Sheets Extension - TypeScript + Rollup",
  "version": "1.0.0",
  "author": "Grasp",
  "main": "index.js",
  "scripts": {
    "compile": "rollup --config",
    "push": "clasp push --force",
    "watch": "npm-run-all --parallel watch:compile watch:push",
    "watch:compile": "npm run compile -- --watch",
    "watch:push": "npm run push -- --watch",
    "build": "npm run compile",
    "dev": "npm run watch",
    "login": "clasp login",
    "create": "clasp create --type sheets --title \"Grasp Final\"",
    "open": "clasp open"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.27.6",
    "@google/clasp": "^2.4.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/google-apps-script": "^1.0.47",
    "@types/node": "^20.10.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.75.5",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "google-sheets",
    "typescript",
    "rollup"
  ],
  "license": "ISC"
}
